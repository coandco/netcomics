#-*-Perl-*-

$hof{"cad"} = 0;

sub cad {
    my $time = shift(@_);
    my @ltime = gmtime($time);
    
    my $date = strftime("%Y%m%d",@ltime);
    
    return undef unless $date > 20021022;
    
    my $base = 'http://www.cad-comic.com/';

    my $rec = {
	'title' => 'Ctrl-Alt-Del',
	'author' => 'Tim Buckley',
	'type' => 'jpg',
	'main' => $base,
	'base' => $base,
	'archives' => $base . 'cad/archive',
	'page' => "cad/$date",
	'exprs' => ["/(comics/cad/$date\.jpg)"],
	};
    return $rec;
}

1;
